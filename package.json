{
   "name": "typeorm-sample",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/cookie-parser": "^1.4.4",
      "@types/express": "^4.17.18",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "cookie-parser": "^1.4.6",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-async-errors": "^3.1.1",
      "jsonwebtoken": "^9.0.2",
      "moment": "^2.29.4",
      "multer": "^1.4.5-lts.1",
      "mysql": "^2.18.1",
      "nodemon": "^3.0.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "dev": "nodemon --watch *.ts --exec ts-node src/index.ts",
      "migration:create": "typeorm migration:create",
      "migration:run": "npx typeorm-ts-node-commonjs migration:run  -d src/data-source",
      "migration:revert": "npx typeorm-ts-node-commonjs migration:revert  -d src/data-source",
      "migration:generate": "npx typeorm-ts-node-commonjs migration:generate"
   }
}
